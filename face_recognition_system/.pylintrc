[MASTER]
init-hook='import sys; sys.path.append(".")'

[MESSAGES CONTROL]
disable=C0111,C0103,C0303,W0611,W0612,W0613,W0621,W0622,W0702,W0703,R0903,R0912,R0913,R0914,R0915

[TYPECHECK]
ignored-modules=setuptools
ignored-classes=setuptools.dist.Distribution

[SIMILARITIES]
min-similarity-lines=7
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=yes
